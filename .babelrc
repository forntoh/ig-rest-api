{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "production": {
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env"
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env.test"
          }
        ]
      ]
    }
  }
}
